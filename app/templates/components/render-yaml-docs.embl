= each parsedDocs as |doc|
  .grid-x
    .small-12.medium-4.large-3.cell.divider
    .small-12.medium-8.large-9.cell.right-side.divider

  .grid-x
    .small-12.medium-4.large-3.cell.left-side
      h5 = doc.title

      = each (split-into-paragraphs doc.docs) as |paragraph|
        p == parse-markdown paragraph

    .small-12.medium-8.large-9.cell.right-side
      = if doc.emblem
        = emblem-handlebars code=doc.emblem
      = if doc.shell
        pre
          code.emblem
            = doc.shell
